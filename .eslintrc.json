{
    "extends": "airbnb-base",
    "env": {
        "es6": true,
        "commonjs": true
    },
    "rules": {
        "quotes": ["warn", "single"],
        "semi": "warn",
        "no-console": "off",
        "react/forbid-prop-types": "off",
        "no-undef": "warn",
        "comma-dangle": ["warn", "always-multiline"]
    }
}